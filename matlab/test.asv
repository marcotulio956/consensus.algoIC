X=[1,3,4,9]';
dt=0.001;

Adj=[
    0 1 1 1; 
    1 0 1 1; 
    1 1 0 1; 
    1 1 1 0
];

Din=eye(4);
Lagran=Din-Adj;

Lagran

for row = i:4
    sumDx = 0
    for col = 1:4
        if col ~= row
            sumDx = sumDx + (Lagran(row,col)*X(row))
        end
    end
    X(row)=X(row)+sumDx*dt
end

X